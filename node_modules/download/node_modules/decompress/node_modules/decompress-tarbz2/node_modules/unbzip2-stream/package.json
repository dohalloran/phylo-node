{
  "name": "unbzip2-stream",
  "version": "1.0.10",
  "description": "streaming unbzip2 implementatio in pure javascript for node and browsers",
  "keywords": [
    "bzip",
    "bzip2",
    "bz2",
    "stream",
    "streaming",
    "decompress",
